{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [ 
    "resources/**/*.ts",
    "resources/**/*.d.ts",
    "resources/**/*.vue"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "types": ["vite/client"], 
    "paths": {
      "@/*": ["resources/js/*"]
    }
  },
  "vueCompilerOptions": {
    "plugins": [
      "@volar/vue-language-plugin-pug"
    ]
  }
}